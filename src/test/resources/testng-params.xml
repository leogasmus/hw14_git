<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Calculator Suit">
    <listeners>
        <listener class-name="Listener"/>
    </listeners>
    <test name="Test parameters from testng file">
        <classes>
            <class name="CalculatorTest">
                <parameter name="firstParam" value="5"/>
                <parameter name="secondParam" value="7"/>
                <parameter name="Result" value="12"/>
                <methods>
                    <include name="addTest"/>
                </methods>
            </class>
        </classes>
    </test> <!-- Test -->
</suite> <!-- Suite -->